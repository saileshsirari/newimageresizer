<androidx.core.widget.NestedScrollView  xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:padding="2dp"
    android:background="@android:color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:padding="2dp"
    android:id="@+id/parentDetail"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_gravity="center"
        android:paddingBottom="5dp"

        android:id="@+id/banner_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="vertical">
    </LinearLayout>


    <!--<apps.sai.com.imageresizer.util.NestedWebView
        android:layout_gravity="center"
        android:id="@+id/image_multiple"

        android:scaleType="centerCrop"

        android:layout_width="match_parent"
        android:layout_height="380dp" />-->

    <ImageView
        android:background="@drawable/border_large"
        android:layout_below="@+id/banner_container"
        android:layout_gravity="center"
        android:id="@+id/image_multiple"
        android:padding="7dp"

        android:scaleType="centerCrop"

        android:layout_width="match_parent"
        android:layout_height="450dp" />


    <TextView
        android:gravity="center"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/image_multiple"
        android:id="@+id/text_name_resolution"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:gravity="center"

        android:layout_centerHorizontal="true"

        android:layout_below="@+id/text_name_resolution"
        android:id="@+id/text_name_size"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

   <!-- <LinearLayout
        android:layout_gravity="center"
        android:id="@+id/banner_container_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_below="@+id/text_name_size"

        android:orientation="vertical">
    </LinearLayout>-->
    <View
        android:layout_width="match_parent"
        android:background="@color/com_facebook_blue"
        android:layout_height="5dp"></View>
    <TextView
        android:visibility="gone"
        android:gravity="center"
        android:id="@+id/seperator"
        android:text="Compressed information:"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/text_name_size"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
 <LinearLayout
     android:id="@+id/parentCompressed"
     android:orientation="vertical"
     android:visibility="gone"
     android:layout_below="@+id/seperator"

     android:layout_width="match_parent"
     android:layout_height="match_parent">

   <!-- <apps.sai.com.imageresizer.util.NestedWebView

        android:layout_gravity="center"
        android:id="@+id/image_multiple_compressed"

        android:scaleType="centerCrop"

        android:layout_width="match_parent"
        android:layout_height="380dp" />-->
     <ImageView
         android:background="@drawable/border_large"

         android:layout_gravity="center"
         android:id="@+id/image_multiple_compressed"

         android:scaleType="centerCrop"

         android:layout_width="match_parent"
         android:layout_height="400dp" />

    <TextView
        android:gravity="center"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/seperator"
        android:id="@+id/text_name_compressed_resolution"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:paddingBottom="130dp"
        android:gravity="center"

        android:layout_centerHorizontal="true"
        android:layout_gravity="center"

        android:layout_below="@+id/text_name_compressed_resolution"
        android:id="@+id/text_name_compressed_size"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"



        />


 </LinearLayout>

    <ProgressBar
        android:visibility="gone"
        android:layout_gravity="center"
        android:id="@+id/progressMyImages"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</RelativeLayout>
</androidx.core.widget.NestedScrollView>
    <!--</androidx.cardview.widget.CardView>-->


